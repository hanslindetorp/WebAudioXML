<?xml version="1.0" encoding="UTF-8"?>
<Audio version="1.0" timeUnit="ms" id="Oscillator">
    <var name="decay" mapin="0,1" mapout="10,1000"></var>

    <Chain>
        <OscillatorNode type="type.txt" frequency="frequency.txt"></OscillatorNode>
        <BiquadFilterNode>
            <frequency value="$filter">
                <!-- <Envelope class="bang" times="0,1000" values="1000, 0"></Envelope> -->
            </frequency>
        </BiquadFilterNode>
        <GainNode>
            <gain>                
                <Envelope class="bang" times="0, $decay" values="env-values.csv"></Envelope>

                <!-- <Envelope class="bang" times="(env-times.js)(40)" values="env-values.csv"></Envelope> -->
            </gain>
        </GainNode>
        <GainNode gain="$gain"></GainNode>
    </Chain>
    
</Audio>