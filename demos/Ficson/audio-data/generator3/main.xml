            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*3" Q="$Q*3"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*4" Q="$Q*4"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*5" Q="$Q*5"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*6" Q="$Q*6"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*7" Q="$Q*7"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*8" Q="$Q*8"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*9" Q="$Q*9"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*10" Q="$Q*10"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*11" Q="$Q*11"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*12" Q="$Q*12"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*13" Q="$Q*13"></BiquadFilterNode>
            <BiquadFilterNode class="filter3" type="bandpass" frequency="$root*14" Q="$Q*14"></BiquadFilterNode>
        </Mixer>
    </Chain>
</Audio>