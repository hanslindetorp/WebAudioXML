<?xml version="1.0" encoding="UTF-8"?>
<Audio version="1.0" timeUnit="ms" gain="-6dB" transitionTime="20">
	<var name="frequency" value="$hand_fingersup - 1" mapin="0,10" mapout="60,77" steps="0,2,4,5,7,9,11,12" convert="MIDI->frequency"></var>
	<var name="gain" value="$hand_fingersup" mapin="0,1" mapout="0,1"></var>
	
	<Chain>
		<OscillatorNode type="sine" frequency="$frequency"></OscillatorNode>
		<GainNode gain="$gain"></GainNode>
	</Chain>
	
</Audio>