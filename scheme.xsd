    <xs:attribute name="value" type="value"/>
    <xs:attribute name="src" type="src"/>
    <xs:attribute name="href" type="href"/>

    <xs:attribute name="gain" type="gain"/>
    <xs:attribute name="type" type="gain"/>
    <xs:attribute name="frequency" type="gain"/>
    <xs:attribute name="outputBus" type="gain"/>
    <xs:attribute name="pan" type="pan"/>

    <xs:attribute name="ratio" type="ratio"></xs:attribute>
  </xs:attributeGroup>

  <!-- Elements -->

  <xs:element name="Audio">
    <xs:complexType>
      <xs:group ref="waxml" minOccurs="0" maxOccurs="unbounded"/>
      <xs:attributeGroup ref="waxml"/>
    </xs:complexType>
  </xs:element>

