      <xs:element name="ChannelMergerNode">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="1" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="ChannelSplitterNode">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="1" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>


