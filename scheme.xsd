          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>
      
      <xs:element name="include">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>



      <!-- WAXML routing -->
      <xs:element name="Send">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="Chain">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="0" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>
      
      <xs:element name="Mixer">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="0" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      
      <!-- WAXML routing -->
      <xs:element name="Synth">
        <xs:complexType>
          <xs:choice>
            <xs:element name="Voice" minOccurs="1" maxOccurs="unbounded"></xs:element>
          </xs:choice>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="ObjectBasedAudio">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="AmbientAudio">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      
    </xs:choice>   
