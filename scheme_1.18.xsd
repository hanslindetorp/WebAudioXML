        </xs:complexType>
      </xs:element>

      <xs:element name="ConvolverNode">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="DynamicsCompressorNode">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      

      <!-- Native elements with children -->
      <xs:element name="ChannelMergerNode">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="1" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="ChannelSplitterNode">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="1" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>


      <!-- WAXML logic -->
      <xs:element name="var">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="Envelope">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>
      
      <xs:element name="include">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>
      




      <!-- WAXML routing -->
      <xs:element name="Send">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="Chain">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="0" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>
      
      <xs:element name="Mixer">
        <xs:complexType>
          <xs:group ref="waxml" minOccurs="0" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      
      <!-- WAXML objects -->
      <xs:element name="Synth">
        <xs:complexType>
          <xs:choice>
            <xs:element name="Voice" minOccurs="1" maxOccurs="unbounded"></xs:element>
          </xs:choice>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="ObjectBasedAudio">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="AmbientAudio">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>


      <xs:element name="Snapshot">
        <xs:complexType>
          <xs:group ref="snapshot" minOccurs="1" maxOccurs="unbounded"/>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>


      <xs:element name="Noise">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      
    </xs:choice>   
  </xs:group>
  

