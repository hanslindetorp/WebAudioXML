        </xs:complexType>
      </xs:element>
    </xs:choice>
  </xs:group>


  <xs:group name="waxml">
    <xs:choice>


      <!-- Native elements -->
      <xs:element name="AudioBufferSourceNode">
        <xs:complexType>
          <xs:attributeGroup ref="waxml"/>
        </xs:complexType>
      </xs:element>

      <xs:element name="AudioWorkletNode">
